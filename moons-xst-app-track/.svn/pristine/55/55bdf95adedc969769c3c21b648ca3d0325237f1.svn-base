package com.moons.xst.track.bean;

import java.io.Serializable;

public class ReturnResultInfo implements Serializable {
	private boolean Result_YN = false;
	private String Result_Content_TX = "";
	private String Error_Message_TX = "";
	private String ExResult1_TX = "";
	private String ExResult2_TX = "";
	private String ExResult3_TX = "";
	private String ExResult4_TX = "";
	private String ExResult5_TX = "";
	
	public ReturnResultInfo() {}
	
	public ReturnResultInfo(boolean result_YN, String result_content_tx, String error_message_tx,
			String exresult1_tx, String exresult2_tx,String exresult3_tx,String exresult4_tx,String exresult5_tx) {
		Result_YN = result_YN;
		Result_Content_TX = result_content_tx;
		Error_Message_TX = error_message_tx;
		ExResult1_TX = exresult1_tx;
		ExResult2_TX = exresult2_tx;
		ExResult3_TX = exresult3_tx;
		ExResult4_TX = exresult4_tx;
		ExResult5_TX = exresult5_tx;
	}
	
	public boolean getResult_YN() {
		return Result_YN;
	}
	public void setResult_YN(boolean result_YN) {
		Result_YN = result_YN;
	}
	
	public String getResult_Content_TX() {
		return Result_Content_TX;
	}
	public void setResult_Content_TX(String result_Content_TX) {
		Result_Content_TX = result_Content_TX;
	}
	
	public String getError_Message_TX() {
		return Error_Message_TX;
	}
	public void setError_Message_TX(String error_Message_TX) {
		Error_Message_TX = error_Message_TX;
	}
	
	public String getExResult1_TX() {
		return ExResult1_TX;
	}
	public void setExResult1_TX(String exResult1_TX) {
		ExResult1_TX = exResult1_TX;
	}
	
	public String getExResult2_TX() {
		return ExResult2_TX;
	}
	public void setExResult2_TX(String exResult2_TX) {
		ExResult2_TX = exResult2_TX;
	}
	
	public String getExResult3_TX() {
		return ExResult3_TX;
	}
	public void setExResult3_TX(String exResult3_TX) {
		ExResult3_TX = exResult3_TX;
	}
	
	public String getExResult4_TX() {
		return ExResult4_TX;
	}
	public void setExResult4_TX(String exResult4_TX) {
		ExResult4_TX = exResult4_TX;
	}
	
	public String getExResult5_TX() {
		return ExResult5_TX;
	}
	public void setExResult5_TX(String exResult5_TX) {
		ExResult5_TX = exResult5_TX;
	}
}