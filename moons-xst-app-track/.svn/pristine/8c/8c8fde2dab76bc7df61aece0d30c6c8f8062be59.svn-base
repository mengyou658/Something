<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical" >
    <RelativeLayout 
		android:id="@+id/ll_comm_download_errorresultback_head" 
	  	android:layout_width="fill_parent" 
	  	android:layout_height="50dip"
	  	android:background="@drawable/title_bar"> 
	  	<ImageButton 
	        android:id="@+id/home_head_Rebutton"
	        android:src="@drawable/arrow_left"
	        android:background="@color/full_transparent"
	        android:layout_width="wrap_content"
	    	android:layout_height="50dip"
	    	android:layout_alignParentLeft="true"
           	android:layout_gravity="center_vertical"
	        android:layout_marginRight="10dp"
	        />
	    <TextView 
	        android:id="@+id/ll_comm_download_errorresultback_head_title"
		  	android:layout_width="fill_parent" 
		  	android:layout_height="fill_parent"
			android:gravity="center"
	        style="@style/main_head_title"
			android:ellipsize="marquee" 
	        android:marqueeRepeatLimit="marquee_forever"
	        android:focusable="true" 
	        android:focusableInTouchMode="true"					
			
			/>
	</RelativeLayout>
    <com.moons.xst.track.widget.SwipeMenu.SwipeMenuListView
        android:id="@+id/lv_comm_download_errorresultback"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:dividerHeight="1px"
        android:divider="@color/graywhite" />
    <include 
	    layout="@layout/vertical_split_line_10dp"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Tips:"/>
    <RelativeLayout 
		android:id="@+id/ll_comm_download_errorresultback_tips" 
	  	android:layout_width="fill_parent" 
	  	android:layout_height="60dip"
	  	android:background="@color/face_bg" >
	   <Button
	       android:id="@+id/btn_back"
	       android:layout_width="fill_parent"
	       android:layout_height="wrap_content"
	       android:layout_alignParentBottom="true"
	       android:layout_alignParentTop="true"
	       android:layout_margin="5dip"
	       android:background="@drawable/btn_style_blue"
	       android:clickable="true"
	       android:text="@string/commu_receive_btn_download"
	       android:textColor="@color/white"
	       android:textSize="@dimen/text_size_20" />
	</RelativeLayout>
</LinearLayout>
