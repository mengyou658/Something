<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:background="#F0EFF3">
    <RelativeLayout 
		android:id="@+id/layout_statistics_uncheck_head" 
	  	android:layout_width="fill_parent" 
	  	android:layout_height="50dip"
	  	android:background="@drawable/title_bar"> 
        <ImageButton 
	        android:id="@+id/home_head_Rebutton"
	        android:src="@drawable/arrow_left"
	        android:background="@color/full_transparent"
	        android:layout_width="wrap_content"
	    	android:layout_height="50dip"
	    	android:layout_alignParentLeft="true"
           	android:layout_gravity="center_vertical"
	        android:layout_marginRight="10dp"
	        />
	    <TextView 
	        android:id="@+id/statistics_uncheck_head_title"
	        android:layout_width="fill_parent" 
		  	android:layout_height="fill_parent"
			android:gravity="center"
	        style="@style/main_head_title"
			android:ellipsize="marquee" 
	        android:marqueeRepeatLimit="marquee_forever"
	        android:focusable="true" 
	        android:focusableInTouchMode="true"			
			android:text="@string/statistics_uncheck_title"
			/>  
	</RelativeLayout>
	<FrameLayout 
	    android:id="@+id/fl_statistics_uncheck_pie_chart"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent">
	    
	</FrameLayout>
</LinearLayout>