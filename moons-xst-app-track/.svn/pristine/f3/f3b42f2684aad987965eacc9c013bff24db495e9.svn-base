<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="0dp"
    android:background="#00000000" >
 
    <RelativeLayout
        android:id="@+id/fl_home_main_content_right"
        android:layout_width="180dp"
        android:layout_height="match_parent"
        android:layout_alignParentRight="true"
        android:background="#00000000"> 
        <RelativeLayout
        android:id="@+id/fl_home_main_content_right_view2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        android:background="@color/half_transparent_white">
              <RelativeLayout
		        android:id="@+id/fl_home_main_content_right_view1_title"
		        style="@style/pad_home_content_title_style"
		        android:orientation="vertical"
		        >
			    <TextView
			        android:id="@+id/home_txt_tipsdj"
			        android:layout_width="fill_parent"
			        android:layout_height="fill_parent"
			        android:layout_marginLeft="10dip"
			        android:gravity="left|center"
			        android:text="消息提醒"
			        style="@style/pad_home_content_title_txt_style" />
		    </RelativeLayout>
		    <RelativeLayout 
		        android:id="@+id/fl_home_main_content_right_view1_messagelist"
			  	android:layout_width="fill_parent" 
			  	android:layout_height="wrap_content"
			  	android:layout_margin="5dp"
			  	android:layout_below="@id/fl_home_main_content_right_view1_title"
		        >
				<ListView
				    android:id="@+id/home_main_content_right_view1_messagelist"
				    android:layout_width="fill_parent"
				    android:layout_height="fill_parent"
				    android:layout_alignParentLeft="true"
				    android:layout_alignParentTop="true"
				    android:cacheColorHint="#0000"
				    android:divider="#80ffffff"
				    android:dividerHeight="1dp"
				    android:scrollbarStyle="outsideOverlay"
		            android:stackFromBottom="false" >  
		         </ListView>          
		    </RelativeLayout>
        </RelativeLayout> 
    </RelativeLayout> 
    <RelativeLayout
        android:id="@+id/fl_home_main_content_left"
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:layout_toLeftOf="@id/fl_home_main_content_right"
        android:background="#00000000">
        
        <RelativeLayout
        android:id="@+id/fl_home_main_content_left_view2"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="10dp"
        android:layout_alignParentBottom="true"
        android:background="@color/half_transparent_white">
		    <RelativeLayout
		        android:id="@+id/fl_home_main_content_left_view2_title"
		        style="@style/pad_home_content_title_style"
		        android:orientation="vertical"
		        >
			    <TextView
			        android:id="@+id/home_txt_tipsdj1"
			        android:layout_width="fill_parent"
			        android:layout_height="fill_parent"
			        android:layout_marginLeft="10dip"
			        android:gravity="left|center"
			        android:text="快速入口"
			        style="@style/pad_home_content_title_txt_style" />
		    </RelativeLayout>
		    <TableLayout
		            android:id="@+id/fl_home_main_content_left_view2_content"
			        android:layout_width="match_parent"
			        android:layout_height="match_parent"
			        android:gravity="left|center"
			        android:layout_below="@id/fl_home_main_content_left_view2_title"
			        android:layout_marginBottom="2dp" >
			
			        <TableRow
			            android:id="@+id/li_home_main_menuitem_1"
			            android:layout_width="wrap_content"
			            android:layout_height="0dp" >
			            <RelativeLayout
			                android:id="@+id/ll_home"
			            	android:layout_width="30dp"
			            	android:layout_height="55dp"
			              	android:gravity="center"
			              	android:clickable="true"
			              	android:orientation="vertical"
			              	android:layout_weight="1">               	
			                <ImageView
			                    android:id="@+id/pad_home_main_xj"
			                    android:layout_width="25dp"
			                    android:layout_height="25dp"
			                    android:layout_centerHorizontal="true"
			                    android:scaleType="fitCenter"
			                    android:layout_marginTop="10dp"
			                    android:src="@drawable/pad_scan_sbda" />
			                <TextView
			                    android:id="@+id/main_menu_home_txt"
			            		android:text="设备信息"
			            		android:layout_width="fill_parent"
			            		android:layout_height="wrap_content"
			            		android:gravity="center"
			            		android:layout_centerHorizontal="true"
			            		android:layout_alignParentBottom="true"
			            		android:layout_below="@id/pad_home_main_xj"
			            		style="@style/pad_main_quick_menu_name"
			            		/>                
			        	 </RelativeLayout>
			           <RelativeLayout
			                android:id="@+id/ll_home1"
			            	android:layout_width="30dp"
			            	android:layout_height="55dp"
			              	android:gravity="center"
			              	android:clickable="true"
			              	android:orientation="vertical"
			              	android:layout_weight="1">               	
			                <ImageView
			                    android:id="@+id/pad_home_main_xj1"
			                    android:layout_width="25dp"
			                    android:layout_height="25dp"
			                    android:scaleType="fitCenter"
			                    android:layout_centerHorizontal="true"
			                    android:layout_marginTop="10dp"
			                    android:src="@drawable/pad_userinfo" />
			                <TextView
			                    android:id="@+id/main_menu_home_txt1"
			            		android:text="@string/pad_home_sys_menu_userset"
			            		android:layout_width="fill_parent"
			            		android:layout_height="wrap_content"
			            		android:gravity="center"
			            		android:layout_centerHorizontal="true"
			            		android:layout_alignParentBottom="true"
			            		style="@style/pad_main_quick_menu_name"
			            		/>                
			        	 </RelativeLayout>
			            <RelativeLayout
			                android:id="@+id/ll_home2"
			            	android:layout_width="55dp"
			            	android:layout_height="55dp"
			              	android:gravity="center"
			              	android:clickable="true"
			              	android:orientation="vertical"
			              	android:layout_weight="1">
                
			        	 </RelativeLayout>
			            <RelativeLayout
			                android:id="@+id/ll_home3"
			            	android:layout_width="55dp"
			            	android:layout_height="55dp"
			              	android:gravity="center"
			              	android:clickable="true"
			              	android:orientation="vertical"
			              	android:layout_weight="1">
			                
        	 			</RelativeLayout>
        		</TableRow>
    </TableLayout>
        </RelativeLayout>
        
        <RelativeLayout
        android:id="@+id/fl_home_main_content_left_view1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="5dp"
        android:layout_above="@id/fl_home_main_content_left_view2"
        android:background="@color/half_transparent_white">
	          <RelativeLayout
		        android:id="@+id/fl_home_main_content_left_view1_title"
		        style="@style/pad_home_content_title_style"
		        android:orientation="vertical"
		        >
			    <TextView
			        android:id="@+id/fl_home_main_content_left_view1_title_txt"
			        android:layout_width="fill_parent"
			        android:layout_height="fill_parent"
			        android:layout_marginLeft="10dip"
			        android:gravity="left|center"
			        android:text="待办事宜"
			        style="@style/pad_home_content_title_txt_style" />
		    </RelativeLayout>
		    <RelativeLayout 
		        android:id="@+id/fl_home_main_content_left_view1_todo"
			  	android:layout_width="fill_parent" 
			  	android:layout_height="wrap_content"
			  	android:layout_margin="5dp"
			  	android:layout_below="@id/fl_home_main_content_left_view1_title"
		        >
				<ListView
				    android:id="@+id/home_main_content_left_view1_todolist"
				    android:layout_width="fill_parent"
				    android:layout_height="fill_parent"
				    android:layout_alignParentLeft="true"
				    android:layout_alignParentTop="true"
				    android:cacheColorHint="#0000"
				    android:divider="#7fffffff"
				    android:dividerHeight="1dp"
				    android:scrollbarStyle="outsideOverlay"
		            android:stackFromBottom="false" >  
		         </ListView>       
		    </RelativeLayout>
        </RelativeLayout> 
        
    </RelativeLayout>

</RelativeLayout>