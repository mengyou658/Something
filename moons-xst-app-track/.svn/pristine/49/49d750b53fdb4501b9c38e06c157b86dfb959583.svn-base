package com.moons.xst.track.bean;

public class XJ_TaskIDPosHis extends DJ_TaskIDPosActive {
	private String Line_ID;
	private String IDPosDesc_TX;
	
	public String getLine_ID() {
		return Line_ID;
	}

	public void setLine_ID(String line_ID) {
		Line_ID = line_ID;
	}
	
	public String getIDPosDesc_TX() {
		return IDPosDesc_TX;
	}

	public void setIDPosDesc_TX(String idPosDesc_TX) {
		IDPosDesc_TX = idPosDesc_TX;
	}
	
	public XJ_TaskIDPosHis() {}
	
	public XJ_TaskIDPosHis(String Line_ID, String IDPos_ID, String IDPosStart_TM,
			String IDPosDesc_TX, String IDPosEnd_TM, String Query_DT, String AppUser_CD,
			String AppUserName_TX, String ShiftName_TX,
			String ShiftGroupName_TX, int TimeCount_NR, String TransInfo_TX,
			String GPSInfo_TX) {
		this.Line_ID = Line_ID;
		this.IDPos_ID = IDPos_ID;
		this.IDPosStart_TM = IDPosStart_TM;
		this.IDPosDesc_TX = IDPosDesc_TX;
		this.IDPosEnd_TM = IDPosEnd_TM;
		this.Query_DT = Query_DT;
		this.AppUser_CD = AppUser_CD;
		this.AppUserName_TX = AppUserName_TX;
		this.ShiftName_TX = ShiftName_TX;
		this.ShiftGroupName_TX = ShiftGroupName_TX;
		this.TimeCount_NR = TimeCount_NR;
		this.TransInfo_TX = TransInfo_TX;
		this.GPSInfo_TX = GPSInfo_TX;
	}
}