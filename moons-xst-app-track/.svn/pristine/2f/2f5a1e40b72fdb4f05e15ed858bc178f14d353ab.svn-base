package com.moons.xst.track.xstinterface;

import java.util.List;

import android.content.DialogInterface;

/**
 * 普通多选对话框回调接口
 * 
 * @author lkz
 * 
 */
public interface SimpleMultiListViewDialogListener {
	/**
	 * 回调函数，刷新父UI
	 */
	public void refreshParentUI();

	/**
	 * 回调函数，返回父UI
	 */
	public void goBackToParentUI();

	/**
	 * 回调函数，确定返回
	 * 
	 * @param mDJPlan
	 */
	public void btnOK(DialogInterface dialog, List<List<String>> mData);
}
